  "version": "0.1.0",
  "description": "",
  "main": "./init.js",
  "scripts": {
    "dev": "npm run build-dev && npm start",
    "build-dev": "cross-env NODE_ENV=development webpack",
    "start": "cross-env NODE_ENV=development electron .",
    "test": "npm run test-lint && npm run test-mocha",
    "test-lint": "cross-env NODE_ENV=test standard",
    "test-mocha": "cross-env NODE_ENV=test mocha -R spec --compilers js:babel-core/register --require ./test/setup.js test/**/*.spec.js",
    "watch": "cross-env NODE_ENV=test webpack-dev-server"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
